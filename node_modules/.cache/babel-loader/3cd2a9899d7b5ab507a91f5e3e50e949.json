AAJZ,CATa,CAAf;AAsCA,IAAMQ,MAAM,GAAG,IAAIf,SAAJ,CAAc;AAC3BU,EAAAA,MAAM,EAANA;AAD2B,CAAd,CAAf,C,CAIA;;AACAK,MAAM,CAACC,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC;AACA;AACA;AACA;AAEA,MAAIF,EAAE,CAACN,IAAH,KAAY,QAAhB,EAA0B,OAAOQ,IAAI,EAAX,CANU,CAOpC;;AACA,MAAMC,QAAQ,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,OAA9B,CAAjB;AACA,MAAI,CAACH,QAAL,EAAe,OAAOD,IAAI,CAAC,QAAD,CAAX;AACfA,EAAAA,IAAI;AACL,CAXD;AAaA,eAAeJ,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport login from '../components/login'\nimport home from '../components/home'\nimport welcome from '../components/welcome'\nimport Users from '../components/user/Users'\nimport Rights from '../components/power/Rights'\nimport Roles from '../components/power/Roles'\nimport Cate from '../components/goods/Cate'\nimport Params from '../components/goods/Params'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    redirect: '/login'\n  },\n  {\n    path: '/login',\n    component: login\n  },\n  {\n    path: '/home',\n    component: home,\n    redirect: '/welcome',\n    children: [\n      {\n        path: '/welcome',\n        component: welcome\n      },\n      {\n        path: '/users',\n        component: Users\n      },\n      {\n        path: '/rights',\n        component: Rights\n      },\n      {\n        path: '/roles',\n        component: Roles\n      },\n      {\n        path: '/categories',\n        component: Cate\n      }\n    ]\n  }\n]\n\nconst router = new VueRouter({\n  routes\n})\n\n// 挂在路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问的路径\n  // from 代表从哪个路径条状而来\n  // next 是一个函数，表示放行\n  // next() 放行 next('/login') 强制跳转\n\n  if (to.path === '/login') return next()\n  // 获取token\n  const tokenStr = window.sessionStorage.getItem('token')\n  if (!tokenStr) return next('/login')\n  next()\n})\n\nexport default router\n"]}]}